# Specula Environment Variables
# Copy this file to .env and configure as needed

# Base path of the application
# Default: '/' for development, '/Specula/' for GitHub Pages
# Example: '/Specula/' for GitHub Pages deployment
VITE_BASE_PATH=/

# Show example load button
# Set to 'true' to show the example specification load button
# Default: 'false' in development, 'true' in production
VITE_EXAMPLE=false

# Node environment (automatically set by Vite)
# Options: 'development' | 'production'
# This is typically set automatically by the build process
# NODE_ENV=production
